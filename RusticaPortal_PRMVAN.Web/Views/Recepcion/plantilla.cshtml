                    <button type="button" class="btn btn-primary mt-3" data-bs-dismiss="modal" autofocus>Continuar</button>
                </div>
            </div>
        </div>
    </div>

    <script>
        window.addEventListener('DOMContentLoaded', () => {
            const popup = new bootstrap.Modal(document.getElementById('popupBienvenida'));
            popup.show();
        });
    </script>
}

